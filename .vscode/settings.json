{
    "symbolMasks.masks": [
        {
            "language": "cpp",
            "patterns": [
                {
                    "pattern": "/\\*mut\\*/",
                    "replace": "mut",
                    "style": {
                        "fontWeight": "bold"
                    }
                },
                {
                    "pattern": "\\.template ",
                    "replace": ".",
                },
                {
                    "pattern": "\\* _Nullable",
                    "replace": " nullable",
                },
                {
                    "pattern": "\\* _Nonnull",
                    "replace": " nonnull",
                },
                {
                    "pattern": "\\[\\[nodiscard\\]\\] inline constexpr",
                    "replace": "const_fn",
                    "style": {
                        "fontWeight": "bold",
                        "color": "#85874d"
                    }
                },
                {
                    "pattern": "inline constexpr void",
                    "replace": "const_procedure",
                    "style": {
                        "fontWeight": "bold",
                        "color": "#85874d"
                    }
                },
                {
                    "pattern": "inline constexpr explicit",
                    "replace": "const_ctor",
                    "style": {
                        "fontWeight": "bold",
                        "color": "#85874d"
                    }
                }
            ]
        }
    ]
}