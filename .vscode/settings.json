{
    "symbolMasks.masks": [
        {
            "language": "cpp",
            "patterns": [
                {
                    "pattern": "/\\*mut\\*/",
                    "replace": "mut",
                    "style": {
                        "fontWeight": "bold"
                    }
                },
                {
                    "pattern": "\\.template ",
                    "replace": ".",
                },
                {
                    "pattern": "\\* _Nullable",
                    "replace": " nullable",
                },
                {
                    "pattern": "\\* _Nonnull",
                    "replace": " nonnull",
                },
                {
                    "pattern": "::template ",
                    "replace": "::",
                },
                {
                    "pattern": "\\[\\[nodiscard\\]\\] inline( constexpr)?",
                    "replace": {
                        "[[nodiscard]] inline constexpr": {
                            "text": "const_fn",
                            "fontWeight": "bold",
                            "color": "#85874d"
                        },
                        "[[nodiscard]] inline": {
                            "text": "fn",
                            "fontWeight": "bold",
                            "color": "#85874d"
                        },
                    }
                },
                {
                    "pattern": "inline( constexpr)? void",
                    "replace": {
                        "inline constexpr void": {
                            "text": "const_proc",
                            "fontWeight": "bold",
                            "color": "#85874d"
                        },
                        "inline void": {
                            "text": "proc",
                            "fontWeight": "bold",
                            "color": "#85874d"
                        },
                    }
                }
            ]
        }
    ]
}